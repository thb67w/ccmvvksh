pipeline {
  agent any
  //def branch = env.BRANCH_NAME
  //echo "branch1  = ${branch}"
  stages {
    stage("Hello") {
      steps {
        echo "Hello"
      }
    }
    stage("Branch Test") {
      when {
        // skip this stage unless branch is NOT master
        expression {
          return (env.BRANCH_NAME != "develop")
        }
      }
      steps {
        echo "World"
        echo "Heal it"
      }
    }
  }
}